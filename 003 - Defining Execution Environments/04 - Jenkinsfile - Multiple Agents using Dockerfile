pipeline {
	agent none
	stages{
		stage('Stage-1'){
		agent { 
			dockerfile {
				filename '04MavenDockerfile'
				dir '003 - Defining Execution Environments'
				}
			}
			steps { sh 'mvn --version'
			}
			
		stage('Stage-2'){
		agent { 
			dockerfile {
				filename '04MavenDockerfile'
				dir '003 - Defining Execution Environments'
				}
			}
	}
			steps { sh 'npm --version'
			}		

    }
}
